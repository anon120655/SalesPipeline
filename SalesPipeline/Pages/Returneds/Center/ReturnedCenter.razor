
@page "/returned/center"

<PageTitle>รายการที่ผู้จัดการศูนย์ส่งคืน</PageTitle>

<MessageError errorMessage="@_errorMessage" />

@if (_permission.IsView)
{
	<div class="container-fluid">
		<div class="row">
			<div class="col-12 my-2">
				<div class="box_content">
					<div class="d-flex flex-row align-items-center mb-3 border-bottom">
						<div class="p-2"><img src="/image/icon/assign_user128x.png" width="55" /></div>
						<div class="p-2">
							<p class="back_topic">รายการที่ <span class="back_topic_active">ผู้จัดการศูนย์ส่งคืน</span></p>
						</div>
					</div>
					<div class="row row-cols-1 row-cols-sm-2 row-cols-md-3 row-cols-xl-5 row-cols-xxl-6 my-4 filter_s">
						<div class="col mb-2">
							<label class="form-label">ชื่อลูกค้า</label>
							<div class="input-group">
								<input class="form-control" type="search" placeholder="">
								<span class="input-group-append ms-0">
									<button class="btn rounded-start-0 border border-start-0" type="button">
										<i class="fa fa-search"></i>
									</button>
								</span>
							</div>
						</div>
						<div class="col mb-2">
							<label class="form-label">ชื่อผู้ติดต่อ</label>
							<div class="input-group">
								<input class="form-control" type="search" placeholder="">
								<span class="input-group-append ms-0">
									<button class="btn rounded-start-0 border border-start-0" type="button">
										<i class="fa fa-search"></i>
									</button>
								</span>
							</div>
						</div>
						<div class="col mb-2">
							<label class="form-label">ห่วงโซ่</label>
							<select class="selectpicker" data-style="select_btn_set" data-live-search="true">
								<option selected>ทั้งหมด</option>
							</select>
						</div>
						<div class="col mb-2">
							<label class="form-label">ประเภทธุรกิจ</label>
							<select class="selectpicker" data-style="select_btn_set" data-live-search="true">
								<option selected>ทั้งหมด</option>
							</select>
						</div>
						<div class="col mb-2">
							<label class="form-label">ศูนย์สาขา</label>
							<select class="selectpicker" data-style="select_btn_set" data-live-search="true">
								<option selected>ทั้งหมด</option>
							</select>
						</div>
						<div class="col mb-2">
							<label class="form-label">จังหวัด</label>
							<select class="selectpicker" data-style="select_btn_set" data-live-search="true">
								<option selected>ทั้งหมด</option>
							</select>
						</div>
						<div class="col mb-2">
							<label class="form-label">วันที่ส่งคืน</label>
							<select class="selectpicker" data-style="select_btn_set" data-live-search="true">
								<option selected>ทั้งหมด</option>
							</select>
						</div>
						<div class="col mb-2">
							<label class="form-label">เหตุผล</label>
							<select class="selectpicker" data-style="select_btn_set" data-live-search="true">
								<option selected>ทั้งหมด</option>
							</select>
						</div>
						<div class="col mb-2 mt-3">
							<button type="button" class="btn btn-primary text-white mw_7r w-100">
								<i class="fa fa-search fa-xs"></i> ค้นหา
							</button>
						</div>
					</div>
					<div class="row mt-3">
						<div class="col-12">
							<div class="table-responsive border-top">
								<table class="table table_list">
									<thead>
										<tr>
											<th class="text-center">ลำดับที่</th>
											<th class="text-center">ชื่อลูกค้า</th>
											<th class="text-center">ผู้ติดต่อ</th>
											<th class="text-center">ห่วงโซ่</th>
											<th class="text-center">ประเภทธุรกิจ</th>
											<th class="text-center">ศูนย์สาขา</th>
											<th class="text-center">จังหวัด</th>
											<th class="text-center">วันที่ส่งคืน</th>
											<th class="text-center">เหตุผล</th>
											<th class="text-center">รายละเอียด</th>
											<th class="text-center">จัดการ</th>
										</tr>
									</thead>
									<tbody>
										
										@for (int i = 1; i < 6; i++)
										{
											<tr>
												<td class="text-center">@(i)</td>
												<td class="">บริษัท ทดสอบ จำกัด</td>
												<td class="text-center">ชูใจ โชคดี</td>
												<td class="text-center">ผลิต</td>
												<td class="text-center">อุสาหกรรม</td>
												<td class="text-center">ศูนย์สมุทสาคร</td>
												<td class="text-center">ราชบุรี</td>
												<td class="text-center">15/05/66</td>
												<td class="text-center">ปิดกิจการ</td>
												<td class="text-center">
													<a href="/returned/center/detail/@(Guid.NewGuid())" class="fw-bold">
														รายละเอียด
													</a>
												</td>
												<td class="text-center">
													<a class="btn btn-info btn-circle">
														<i class="fa-solid fa-eye text-white fs_15e"></i>
													</a>
												</td>
											</tr>
										}
									</tbody>
								</table>
							</div>
						</div>
					</div>
					<div>
						<PaginationMockup></PaginationMockup>
					</div>
				</div>
			</div>
		</div>
	</div>
}
else
{
	<NotPermissionPage />
}




